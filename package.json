{
  "name": "file-encrypter-decrypter",
  "version": "1.0.0",
  "description": "An app to encrypt and decrypt files.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "Prashanta Mondal",
  "license": "ISC",
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^24.6.3"
